{
  "name": "spacex-api",
  "version": "2.1.0",
  "description": "Open Source REST API for SpaceX company, vehicle, launchpad, and launch data",
  "main": "app.js",
  "scripts": {
    "test": "PORT=0 jest -i --forceExit && eslint \"**/*.js\"",
    "start": "node src/app.js",
    "lint": "eslint \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-spacex/SpaceX-API"
  },
  "keywords": [
    "spacex",
    "rest-api",
    "mongodb",
    "expressjs"
  ],
  "author": "Jake Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r-spacex/SpaceX-API/issues"
  },
  "homepage": "https://github.com/r-spacex/SpaceX-API",
  "dependencies": {
    "codecov": "^2.3.0",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-rate-limit": "^2.9.0",
    "helmet": "^3.8.1",
    "lower-case": "^1.1.4",
    "mongodb": "^2.2.31",
    "morgan": "^1.8.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "jest": "^20.0.4",
    "supertest": "^3.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  }
}
