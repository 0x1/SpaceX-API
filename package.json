{
  "name": "spacex-api",
  "version": "2.1.0",
  "description": "Open Source REST API for SpaceX company, vehicle, launchpad, and launch data",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=test PORT=0 NODE_ENV=test jest -i --forceExit && eslint \"**/*.js\"",
    "start": "node src/app.js",
    "lint": "eslint \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-spacex/SpaceX-API"
  },
  "keywords": [
    "spacex",
    "rest-api",
    "mongodb",
    "expressjs"
  ],
  "author": "Jake Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r-spacex/SpaceX-API/issues"
  },
  "homepage": "https://github.com/r-spacex/SpaceX-API",
  "dependencies": {
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-async-handler": "^1.0.2",
    "helmet": "^3.8.1",
    "lower-case": "^1.1.4",
    "mongodb": "^2.2.31",
    "morgan": "^1.8.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "jest": "^20.0.4",
    "supertest": "^3.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  }
}
