    "moment-timezone": "^0.5.23",
    "mongodb": "^3.1.13",
    "path": "^0.12.7",
    "path-to-regexp": "^3.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "shelljs": "^0.8.3"
