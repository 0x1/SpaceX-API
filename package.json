    "eslint-plugin-mongodb": "^1.0.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "pkg": "^4.3.7",
    "supertest": "^3.4.2"
