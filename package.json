{
  "name": "spacex-api",
  "version": "2.4.0",
  "description": "Open Source REST API for rocket, core, capsule, pad, and launch data",
  "main": "./src/app.js",
  "scripts": {
    "test": "eslint \"**/*.js\" && NODE_ENV=test PORT=0 jest --silent --forceExit",
    "start": "node src/app.js",
    "lint": "eslint \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-spacex/SpaceX-API"
  },
  "keywords": [
    "spacex",
    "space",
    "rocket",
    "rest-api",
    "mongodb",
    "expressjs"
  ],
  "author": "Jake Meyer",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/r-spacex/SpaceX-API/issues"
  },
  "homepage": "https://github.com/r-spacex/SpaceX-API",
  "dependencies": {
    "koa": "^2.5.0",
    "koa-compress": "^2.0.0",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-redis-cache": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa2-cors": "^2.0.5",
    "lower-case": "^1.1.4",
    "mongodb": "^3.0.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "supertest": "^3.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  }
}
